<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidnetexample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.example.androidnetexample.ToDoController"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.androidnetexample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name="com.example.androidnetexample.CalculatorActivity">
        </activity>
        <activity
            android:name="com.example.androidnetexample.TestPalindromeActivity">
        </activity>
        <activity 
            android:name="com.example.androidnetexample.SearchActivity">
        </activity>
        <activity 
            android:name="com.example.androidnetexample.ToDosActivity">
        </activity>
        <activity
            android:name="com.example.androidnetexample.ToDoDetailActivity">
        </activity>
    </application>

</manifest>
